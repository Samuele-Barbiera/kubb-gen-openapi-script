{
  "name": "kubb-gen-scribe-cli",
  "type": "module",
  "license": "MIT",
  "version": "0.0.1",
  "keywords": [
    "kubb-gen-scribe-cli",
    "init.tips",
    "kubb",
    "scribe",
    "openapi",
    "typescript"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Samuele-Barbiera/kubb-gen-scribe-cli.git"
  },
  "bin": {
    "kubb-gen-scribe-cli": "./dist/index.js"
  },
  "files": [
    "dist",
    "template",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "package.json"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "preinstall": "npx only-allow bun",
    "release": "bun run build && changeset version && changeset publish",
    "lint": "bunx tsc",
    "check": "bunx @biomejs/biome check --apply-unsafe .",
    "update-deps": "bunx npm-check-updates -i --format group -u && bun i",
    "commit": "git add . && npx cz  && git pull",
    "typecheck": "tsc",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist  node_modules",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@biomejs/biome": "1.6.3",
    "@changesets/cli": "^2.27.1",
    "@clack/core": "^0.3.2",
    "@clack/prompts": "^0.6.3",
    "@kubb/cli": "^2.11.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "chalk": "5.2.0",
    "commander": "^10.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-emoji": "1.3.2-canary.2",
    "execa": "^7.1.1",
    "fs-extra": "^11.2.0",
    "gradient-string": "^2.0.2",
    "js-yaml": "^4.1.0",
    "ora": "6.3.1",
    "react": "^18.2.0",
    "sort-package-json": "^2.9.0",
    "tsup": "^8.0.2"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@kubb/core": "^2.11.0",
    "@kubb/swagger": "^2.11.0",
    "@kubb/swagger-tanstack-query": "^2.11.0",
    "@kubb/swagger-ts": "^2.11.0",
    "@types/bun": "latest",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^18.2.72",
    "type-fest": "^4.14.0"
  },
  "peerDependencies": {
    "typescript": "^5.4.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  },
  "cKubbSwaggerCliaMetadata": {
    "initVersion": "0.0.1"
  }
}
