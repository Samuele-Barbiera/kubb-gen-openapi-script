{
	"name": "kubb-gen-scribe-cli",
	"type": "module",
	"license": "MIT",
	"version": "0.0.1",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"scripts": {
		"preinstall": "npx only-allow bun",
		"build": "tsup index.ts --format cjs,esm --dts",
		"release": "bun run build && changeset publish",
		"lint": "bunx tsc",
		"check": "bunx @biomejs/biome check --apply-unsafe .",
		"update-deps": "bunx npm-check-updates -i --format group -u && bun i",
		"commit": "git add . && npx cz  && git pull"
	},
	"devDependencies": {
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5.4.3"
	},
	"dependencies": {
		"@biomejs/biome": "1.6.3",
		"@changesets/cli": "^2.27.1",
		"@kubb/cli": "^2.11.0",
		"@kubb/core": "^2.11.0",
		"@kubb/swagger": "^2.11.0",
		"@kubb/swagger-tanstack-query": "^2.11.0",
		"@kubb/swagger-ts": "^2.11.0",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/js-yaml": "^4.0.9",
		"@types/react": "^18.2.72",
		"cz-conventional-changelog": "^3.3.0",
		"cz-emoji": "1.3.2-canary.2",
		"js-yaml": "^4.1.0",
		"react": "^18.2.0",
		"tsup": "^8.0.2"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-emoji"
		}
	}
}
